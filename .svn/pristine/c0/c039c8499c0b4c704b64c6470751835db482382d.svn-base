/*倒入试卷编辑题目页*/
.zy_cover{padding: 20px;}
.zy_paper_title{padding: 0 !important; border: 1px solid #e0e2e3; border-radius: 5px;}
.zy_paper_title .tit{box-sizing: border-box; height: 48px; padding: 20px 20px 0; background: #efefef; border: none;}
.zy_paper_title .open_analysis{}
.zy_paper_title .open_analysis i{}
.zy_paper_title .upload_box{height: 200px; line-height: 200px; padding: 20px; text-align: center;}
.zy_paper_title .upload_box .upload{}
.zy_paper_title .upload_box .upload label{color: #5093e1; cursor: pointer;}
.zy_paper_title .upload_box .upload input[type=file]{display: none; cursor: pointer;}
.zy_paper_title_btn{margin: 8px 0; text-align: center;}
.zy_paper_title_btn button{width: auto; height: 38px; line-height: 38px; padding: 0 10px; margin: 0 10px;}
.zy_paper_title_btn .cancle{background: #ededed;}
.zy_paper_title_btn .middle{color: #5093e1; border: 1px solid #5093e1; background: #fff;}
.zy_paper_title_btn .sure{color: #fff; background: #5093e1;}

/*倒入试卷编辑卷面*/
.zy_paper_main{padding: 20px 20px 50px;}
.zy_paper_main .tit{padding-bottom: 6px; margin-bottom: 16px; border-bottom: 1px solid #e5e5e5;}
.zy_paper_main .tit span{font-size: 16px;}
.zy_paper_main .tit ul{}
.zy_paper_main .tit ul li{float: left; color: #26b295; margin-left: 30px; cursor: pointer;}

.zy_paper_main .task_list{}
.zy_paper_main .big_question_list{margin-bottom: 10px;}
.zy_paper_main .task_list .head{position: relative; padding: 22px 10px 10px;}
.zy_paper_main .task_list .head .btn{right: 18px;}
.zy_paper_main .task_list .head.active{background: #EFEFEF;}
.zy_paper_main .task_list .head:hover{background: #EFEFEF;}
.zy_paper_main .task_list .head .tit_num{}
.zy_paper_main .task_list .head .tit_cont{width: 1100px;}

.zy_paper_main .task_list .head .edit_big_tit{/*display: none; */width: 1100px;}
.zy_paper_main .task_list .head .edit_big_tit textarea{box-sizing: border-box; width: 84%; height: 50px; padding: 2px 6px; line-height: 22px; border: 1px solid #eee; outline: none;}
.zy_paper_main .task_list .head .edit_big_tit .btn_group{margin: 22px 0 0 20px;}
.zy_paper_main .task_list .head .edit_big_tit .btn_group button{width: 64px; height: 28px}
.zy_paper_main .task_list .head .edit_big_tit .btn_group button.sure{background: #1abc9c; color: #fff;}
.zy_paper_main .task_list .head .edit_big_tit .btn_group button.cancel{background: #f0f0f0; border-color: #f0f0f0; color: #4f5355;}

.zy_paper_main .btn{/*display: none;*/ position: absolute; top: 0; right: 0; height: 22px; line-height: 22px; color: #5093e1; background: #EFEFEF;}
.zy_paper_main .btn b{display: inline-block; padding: 0 10px; margin: 0 2px; text-align: center; font-weight: normal; cursor: pointer;}

.zy_paper_main .task_list .cont{}

.zy_paper_main .task_list .cont{position: relative;}
.zy_paper_main .task_list .cont>li .group_head{position: relative; margin-bottom: 10px;}
.zy_paper_main .task_list .cont>li .group_head .tit_cont{min-height: 38px; padding: 20px 10px; line-height: 24px; background: #efefef;}
.zy_paper_main .task_list .cont>li .group_head .editor_unified{/*display: none;*/ padding-top: 22px;}

.zy_paper_main .task_list .cont>li{position: relative; padding: 10px 10px; margin-bottom: 10px; border: 1px solid transparent; border-radius: 5px;}
.zy_paper_main .task_list .cont>li.active{border-color: #5093e1;}

.zy_paper_main .task_list .cont>li .question{position: relative; padding: 30px 24px 10px; border: 1px solid transparent; border-radius: 5px;}
.zy_paper_main .task_list .cont>li .question:hover{border-color: #ddd;}
.zy_paper_main .task_list .cont>li .question.active{border-color: #ddd;}

.zy_paper_main .task_list .cont>li .question_option{margin-bottom: 10px;}
.zy_paper_main .task_list .cont>li .question_option .question_type>b{position: absolute; left: 0; top: 6px; font-weight: normal; width: 24px; height: 24px; line-height: 24px; text-align: center; background: #666; color: #fff;}
.zy_paper_main .task_list .cont>li .question_option .question_type select{height: 30px; padding: 0 10px; margin-left: 6px; border: 1px solid #e5e5e5; border-radius: 5px; outline: none; cursor: pointer;}
.zy_paper_main .task_list .cont>li .question_option .question_type .question_template{max-width: 84%; margin-left: 16px;}
.zy_paper_main .all_tem{/*display: none;*/width: 750px;}
.zy_paper_main .all_tem>span{line-height: 28px;}
.zy_paper_main .all_tem ul{margin: 0 10px;}
.zy_paper_main .all_tem ul li:not(.option_btn){float: left; box-sizing: border-box; width: 28px; height: 28px; line-height: 28px; text-align: center; margin: 0 4px 4px; border: 1px solid #3f3f3f; border-radius: 50%; cursor: pointer;}
.zy_paper_main .all_tem ul li.active{color: #fff; background: #5093e1; border-color: transparent;}
.zy_paper_main .all_tem ul li.option_btn{margin-left: 10px; line-height: 28px;}
.zy_paper_main .all_tem ul li.option_btn i{font-size: 20px; color: #5BAC84; font-weight: bold; cursor: pointer;}
.zy_paper_main .all_tem ul li.option_btn i:last-of-type{color: #F594AA; }
